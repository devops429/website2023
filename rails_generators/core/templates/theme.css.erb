<%= file_name %>

body
  font-family: 'Lucida Grande'
  font-size: 11pt
  font-weight: normal
  background: #382C2C url(../images/<%= file_name %>/page_bg.gif) center repeat-y

<%= file_name %>

#wrapper
  margin: 0 auto
  width: 1000px

#header
  margin: 0
  padding: 0

#logo
  margin: 24px 0 0 24px


#login_menu
  margin: -8px 16px 0 0
  float: right
  color: #101213

#login_menu a, #login_menu a:visited
  color: #101213
  text-decoration: none
  font-size: 10pt
  padding: 0 8px 0 8px

a.home
  background: url(../images/<%= file_name %>/home_icon.gif) no-repeat
  padding: 0 8px 0 24px
  overflow: visible


.leftmost
  margin-left: 260px

div#top_menu
  :margin-top       1px
  :padding          2px 6px 2px 6px
  :height           29px
  :background       #E3B227 url(../images/<%= file_name %>/top_menu_bg.gif) repeat-y
  :border           1px solid #BC8D03
  :line-height      28px
  :width            50%
  :text-align       center
  :color            #5B470B


#main_menu
  :margin           0 auto
  :height           24px
  :width            1000px
  :padding          8px
  :background       #241C1C

#main_menu a
  :font             normal 15pt 'Lucida Grande', sans-serif
  :text-decoration  none
  :color            #fff
  :margin           0 16px 0 16px

#messages
  margin: 0 28px 0 16px

#messages h1
  font-size: 16pt
  padding: 0px
  margin: 6px 0 6px 0

#messages p
  margin: 6px 0 6px 0
  padding: 0px

.message
  padding: 4px
  margin: 4px
  font-size: 12pt
  text-align: center

.notice
  color: #1d9926
  border-color: #1d9926
  background: transparent url(../images/<%= file_name %>/notice_bg.gif) repeat-x
  border-top: 1px solid
  border-bottom: 1px solid

.warning
  color: #990000
  border-color: #990000
  background: transparent url(../images/<%= file_name %>/warning_bg.gif) repeat-x
  border-top: 1px solid
  border-bottom: 1px solid

.row
  margin-bottom: 4px

.agreement
  color: #626060

.right_info
  float: right
  padding: 16px
  margin: 16px
  border: 1px solid #b8b8b8
  width: 300px

.right_info p
  font-weight: normal


#footer
  margin: 14px auto
  border-top:       1px solid #241C1C
  width: 1000px
  font-family: arial
  font-size: 11pt
  text-align: center

#footer #developer
  float: right
  padding: 12px 12px 0 56px
  margin: 32px 0
  text-align: left
  font-size: 9pt
  font-family: 'lucida grande'
  color: #000
  background: url(../images/<%= file_name %>/railsjitsu_bonsai_black.gif) no-repeat
  height: 40px

#footer #developer:hover
  background: url(../images/<%= file_name %>/railsjitsu_bonsai_red.gif) no-repeat
  color: #e01b1b

#footer #developer a
  color: #000
  text-decoration: none

#pages.index .blue_stripes
  height: 167px
  background: url(../images/<%= file_name %>/blue_stripes.gif) scroll 86% repeat-x
  margin: 0 0 32px 0

#pages.index #homepage_house
  float: left

#pages.index div#agent_search_small
  color: #ffffff
  float: left
  padding: 32px 16px 16px 16px

#pages.index div#agent_search_small label
  color: #ffffff
  font-size: 16pt
  float: none
  text-align: left
  display: block
  margin: 0 0 8px 0

#pages.index div#agent_search_small input[type="text"]
  margin: 0 0 0 32px
  font-size: 14pt
  width: 280px
  padding: 6px
  color: #1aa2d6

#pages.index div#agent_search_small div.field_explanation
  font-size: 10pt
  margin: 0 0 0 80px