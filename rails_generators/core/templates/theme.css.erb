body
  font-family: 'Lucida Grande'
  font-size: 11pt
  font-weight: normal
  background: url(../images/page_bg.gif) repeat-x

#wrapper
  margin: 0 auto
  width: 1000px
  background: #ffffff
  border-left: 1px solid
  border-right: 1px solid
  border-bottom: 1px solid
  border-color: #50bfea

#header
  margin: 0
  padding: 0

#logo
  margin: 24px 0 0 24px


#login_menu
  margin: -8px 16px 0 0
  float: right
  color: #101213

#login_menu a, #login_menu a:visited
  color: #101213
  text-decoration: none
  font-size: 10pt
  padding: 0 8px 0 8px

#login_menu a.home
  background: url(../images/home_icon.gif) no-repeat
  padding: 0 8px 0 24px
  overflow: visible


.leftmost
  margin-left: 260px

#main_menu
  color: #696969

#main_menu ul
  margin: 0
  padding: 0
  font-family: arial
  font-size: 11pt
  border-bottom: 4px solid #49c1f0

#main_menu ul li
  float: left
  list-style: none

#main_menu ul li a
  height: 40px
  display: block
  float: left
  padding-left: 20px
  text-decoration: none

#main_menu li.active
  background: url(../images/menu_tab_left.gif) no-repeat left top

#main_menu ul li a span
  display: block
  font-size: 10pt
  padding: 15px 20px 0 0
  display: block
  height: 40px
  float: left

#main_menu ul li.active a span
  color: #ffffff
  font-weight: bold
  background: url(../images/menu_tab_right.gif) no-repeat right top

#main_menu a, #main_menu a:visited
  color: #696969
  text-decoration: none

#main_menu ul.horizontal_menu
  background: url(../images/main_menu_bg.gif) repeat-x
  height: 40px
  width: 100%
  text-align: center


#messages
  margin: 0 28px 0 16px

#messages h1
  font-size: 16pt
  padding: 0px
  margin: 6px 0 6px 0

#messages p
  margin: 6px 0 6px 0
  padding: 0px

.message
  padding: 4px
  margin: 4px
  font-size: 12pt
  text-align: center

.notice
  color: #1d9926
  border-color: #1d9926
  background: transparent url(../images/notice_bg.gif) repeat-x
  border-top: 1px solid
  border-bottom: 1px solid

.warning
  color: #990000
  border-color: #990000
  background: transparent url(../images/warning_bg.gif) repeat-x
  border-top: 1px solid
  border-bottom: 1px solid



.row
  margin-bottom: 4px

.agreement
  color: #626060

.right_info
  float: right
  padding: 16px
  margin: 16px
  border: 1px solid #b8b8b8
  width: 300px

.right_info p
  font-weight: normal


#footer
  margin: 14px auto
  width: 1000px
  font-family: arial
  font-size: 11pt
  text-align: center

#footer #developer
  float: right
  padding: 12px 0 0 56px
  margin: 32px 0
  text-align: left
  font-size: 9pt
  font-family: 'lucida grande'
  color: #000
  background: url(../images/railsjitsu_bonsai_black.gif) no-repeat
  height: 40px

#footer #developer:hover
  background: url(../images/railsjitsu_bonsai_red.gif) no-repeat
  color: #e01b1b

#footer #developer a
  color: #000
  text-decoration: none

#pages.index .blue_stripes
  height: 167px
  background: url(../images/blue_stripes.gif) scroll 86% repeat-x
  margin: 0 0 32px 0

#pages.index #homepage_house
  float: left

#pages.index div#agent_search_small
  color: #ffffff
  float: left
  padding: 32px 16px 16px 16px

#pages.index div#agent_search_small label
  color: #ffffff
  font-size: 16pt
  float: none
  text-align: left
  display: block
  margin: 0 0 8px 0

#pages.index div#agent_search_small input[type="text"]
  margin: 0 0 0 32px
  font-size: 14pt
  width: 280px
  padding: 6px
  color: #1aa2d6

#pages.index div#agent_search_small div.field_explanation
  font-size: 10pt
  margin: 0 0 0 80px